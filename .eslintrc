{
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 1,

    "comma-dangle": [2, "always-multiline"],
    "constructor-super": [2],
    "eol-last": [2],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "no-alert": [1],
    "no-console": [1],
    "no-debugger": [1],
    "no-trailing-spaces": [2],
    "no-class-assign": [2],
    "no-const-assign": [2],
    "no-this-before-super": [2],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-var": [2],
    "semi": [2, "always"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
